{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "name": {
            "type": "string",
            "enum": [
                "account", "hall", "finder"
            ]
        },
        "age": {
            "type": "integer",
            "minimum": 0,
            "maximum": 24
        }
    },
    "required": [
        "name",
        "age"
    ]
}
